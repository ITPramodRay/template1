{
  "name": "adcat-frontend-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/runtime": "^7.13.10",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "aes-js": "^3.1.2",
    "axios": "^0.21.1",
    "bootstrap": "^5.1.3",
    "infinite-react-carousel": "^1.2.11",
    "jquery": "^3.6.0",
    "react": "^17.0.2",
    "react-collapsible": "^2.8.4",
    "react-date-picker": "^8.3.2",
    "react-dom": "^17.0.2",
    "react-leaf-carousel": "^3.0.0",
    "react-light-accordion": "^0.1.4",
    "react-multi-carousel": "^2.6.5",
    "react-otp-input": "^2.4.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-slick": "^0.28.1",
    "react-tabs": "^3.2.3",
    "react-toastify": "^7.0.4",
    "react-transition-group": "^4.4.2",
    "reactstrap": "^9.0.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "swiper": "^7.3.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "watch": "onchange 'src/**/*.**' 'public/*.**' -- prettier --write {{changed}}",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-sass": "^6.0.1",
    "onchange": "^7.1.0",
    "prettier": "^2.3.2",
    "sass-loader": "^10.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.js",
      "!src/reportWebVitals.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 70,
        "statements": 70
      }
    },
    "coverageReporters": [
      "text",
      "lcov",
      "clover"
    ]
  }
}
